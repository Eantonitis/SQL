/* Cross Join */
SELECT *
FROM table1
CROSS JOIN table2; 

/* Left Join */
SELECT *
FROM table1
LEFT JOIN table2
ON table1.column = table2.column; 

/* Right Join */
SELECT *
FROM table1
RIGHT JOIN table2
ON table1.column = table2.column; 

/*Inner Join */
SELECT *
FROM table1
INNER JOIN table2
ON table1.column = table2.column;

/* Union */
SELECT *
FROM table1
UNION
SELECT *
FROM table2;
